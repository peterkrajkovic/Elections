<Window x:Class="Elections.RemoveParty"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Elections"
        mc:Ignorable="d"
        Title="Remove" Height="250" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBox Name="textBox"  Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10" BorderThickness="0" IsReadOnly="True"/>
        <Button  Content="OK" Width="80" Grid.Row="1"  Height="30" Margin="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="Ok" />
        <ComboBox Name="comboBox" Grid.Row="0" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="10" Width="Auto" Height="Auto" SelectedIndex="0">
        </ComboBox>
    </Grid>
</Window>
