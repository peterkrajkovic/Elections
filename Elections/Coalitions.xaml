<Window x:Class="Elections.Coalitions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Elections"
        mc:Ignorable="d"
        Title="Coalitions" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Canvas Name="canvas" Grid.Column="0"/>
        <Button Grid.Column="1" Content="►" FontSize="25" Background="White" BorderThickness="0" Width="Auto" VerticalAlignment="Center" HorizontalAlignment="Center" Height="Auto" Click="Next"/>
        
    </Grid>
</Window>
